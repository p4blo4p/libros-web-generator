[flake8]
exclude =
    .git,
    __pycache__,
    build,
    dist,
    social
# Puedes añadir más directorios o archivos aquí, separados por comas

# Ejemplo de otras configuraciones que podrías tener
max-line-length = 127
max-complexity = 10
# select = E9,F63,F7,F82  # Si quieres que estas selecciones sean por defecto
